--main
function _init()
	init_map()
	init_player()
	init_items()
	music(0,1)
	start=false
	done=false
end

function _update()
	if not start then
		if btn(‚ùé) then
			start=true
		end
	else
		player_update()
		clean_items()
		check_item_collision()
		if life==0 then
			done=true
		end
	end
end

function _draw()
	cls()
	if not start then
		print("‚Ä¶‚Ä¶üòê‚Ä¶‚Ä¶",10,10,14)
		print("welcome to tokki's fall!", 10,20,7)
		print("tokki fell down a rabbit hole after chasing alice!", 10,30,7)
		print("help tokki avoid dangerous obstacles with ‚¨ÖÔ∏è and ‚û°Ô∏è",10,40,7)
		print("eating cake will make you grow",10,50,7)
		print("drinking a blue bottle will make you shrink",10,60,7)
		print("grabbing jam jars with ‚ùé will give you health",10,70,7)
		print("press ‚ùé to start! good luck!",10,80,7) 
		print("Àáüê±Àá‚ô™",10,90,14)
	else
		draw_map()
		if not done then
			print("points: " ..points, 10, 18, 7)
		end
		draw_player()
	end
end

