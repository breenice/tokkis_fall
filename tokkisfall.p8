--obstacles
function init_items()
	curr_x = 0
	items = {}
	item_ids = {1,2,3,4,5,6,8,9,10,11,12,13,14}
end

function spawn_items()
	for i=1,flr(rnd(3)) + 1 do
		repeat
			x = flr(rnd(97))+11
		until abs(x - curr_x) > 12 -- not too close tgth

		y = posy + 80 -- future off screen

		local item = {
			x = x,
			y = y,
			id = item_ids[flr(rnd(#item_ids)) + 1], -- pick a random sprite
			hit = false
		}

		add(items, item)
		curr_x = x -- update last occupied x position
	end
end


-- destroy after passing
function clean_items() 
	for item in all(items) do
		if not item.hit then
			item.y -= 2

			if item.y > fall_speed+100 then
				del(items, item)
			end
		else
			del(items, item)
		end
	end
end

function check_item_collision()
	for item in all(items) do
		if posx < item.x + 8 and posx + 8 > item.x and 
		   posy < item.y + 8 and posy + 8 > item.y then
		   
			item.hit = true
			if item.id==3 then
				default_sprite=19
			elseif item.id==2 then
				default_sprite=0
			elseif fget(item.id,2) and btn(‚ùé) then
				life+=1
				sfx(15)
			elseif fget(item.id,1)then
				life-=1
				sfx(14)
			else
				player_sprite=15
			end
		end
	end
end


