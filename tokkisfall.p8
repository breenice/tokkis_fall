--map
function init_map()
	wall=0
	fall_speed = 0
end

function draw_map()
	if done then 
		music(-1)
		map(20,0,0,0) 
		print("better luck next time!",20,20)
		return false
	end
	fall_speed += 1
	map(0, fall_speed / 8, 0, 0)

	if fall_speed % 30 == 0 then
		spawn_items()
	end
	
	-- draw from made items
	for item in all(items) do
		if not item.collected then
			spr(item.id, item.x, item.y)
		end
	end

	if fall_speed >= 400 then
		--horiz += 18
		fall_speed = 0
	end
end


