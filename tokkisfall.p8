--player
function init_player()
	posx = 63
	posy = 63
	ttime = 0
	default_sprite = 0
	life=3
	points=0
end

function player_update()
	if done then return false end
	ttime += 1
	if ttime %20==0 then
		points+=1
	end
	if btn(‚¨ÖÔ∏è) then
		if not get_flag(posx-1, posy,0) then
			posx -= 2
		end
	end
	if btn(‚û°Ô∏è) then
		 if not get_flag(posx+7,posy,0) then
				posx += 2
			end
	end
end

function get_flag(x,y,flag)
	local tile_id = mget(flr(x/8),flr(y/8))
	return fget(tile_id, flag) --gets the flag
end

function draw_player()
	for j=0,life-1 do
		spr(25,8+j*10,8)
	end
	if ttime %3==0 then
		if default_sprite==0 then
			player_sprite = (player_sprite==0) and 31 or 0
			size=1
		else
			player_sprite = (player_sprite==19) and 32 or 19
			size=2
		end
	end
	
	if btn(‚ùé) then
		player_sprite=30
	end
	
	spr(player_sprite,posx,posy,size,size)
end

